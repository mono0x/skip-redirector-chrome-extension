{
	"name": "Skip Redirector",
	"version": "1.0.2",
	"description": "Skip some URL redirector and age check.",
	"background_page": "background.html",
	"options_page": "options.html",
	"permissions": [
		"http://wedata.net/"
	],
	"content_scripts": [
		{
			"js"     : ["skip_redirector.js"],
			"matches": ["http://*/*", "https://*/*"]
		}
	]
}
